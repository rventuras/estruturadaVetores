#include <stdio.h>

int main() {
    float n[10], x[10];
    int i;

    printf("Digite 10 n√∫meros:\n");

    for (i = 0; i < 10; i++) {
        scanf("%f", &n[i]);
        x[i] = n[i] * n[i];
    }

    printf("Numeros lidos:\n");

    for (i = 0; i < 10; i++) {
        printf("%.f ", n[i]);
    }

    printf("\nQuadrados dos numeros:\n");

    for (i = 0; i < 10; i++) {
        printf("%.f ", x[i]);
    }

    return 0;
}
