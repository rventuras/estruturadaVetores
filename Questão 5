#include <stdio.h>

int main() {
    int v[10];
    int par = 0;

    
    printf("Digite 10 valores:\n");
    for (int i = 0; i < 10; i++) {
        scanf("%d", &v[i]);
    }

    
    for (int i = 0; i < 10; i++) {
        if (v[i] % 2 == 0) {
            par++;
        }
    }

    
    printf("O vetor possui %d valores pares.\n", par);

    return 0;
}
